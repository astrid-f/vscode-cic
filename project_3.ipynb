    "import plotly.express as px\n",
    "\n",
    "# Create a line chart for Urban-Rural Gap over time\n",
    "fig = px.line(\n",
    "    filtered_poverty_data,\n",
    "    x='Year',\n",
    "    y='Urban-Rural Gap',\n",
    "    color='Entity',\n",
    "    title='Urban-Rural Poverty Gap Over Time',\n",
    "    labels={'Urban-Rural Gap': 'Urban-Rural Poverty Gap (%)', 'Year': 'Year'}\n",
    ")\n",
    "\n",
    "fig.show()\n"
